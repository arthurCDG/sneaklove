<td class="table-division">
  <div><img src="{{this.image}}" alt="img" class="table-image" /></div>
</td>
<td class="table-division">
  <div>{{this.name}}</div>
</td>
<td class="table-division">
  <div>{{this.ref}}</div>
</td>
<td class="table-division">
  <div>${{this.price}}</div>
</td>
<td class="table-division">
  <a href="/dashboard/edit/{{this._id}}"><i class="fas fa-edit"></i></a>
</td>
<td class="table-division">
  <a href="/dashboard/delete/{{this._id}}"><i class="fas fa-trash"></i></a>
</td>